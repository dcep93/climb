<!DOCTYPE html>
<html>
<head>
	<title><%= gym.name %></title>
	<%- include('head.ejs') %>
	<script type="text/javascript" src="/gym.js"></script>
</head>
<body>
	<%- include('signin.ejs') %>
	<a href="/">Home</a>
	<div><%= gym.name %></div>
	<div><%= gym.description %></div>
	<br>
	<div>
		<p>Walls</p>
		<br>
		<% for (var wall of walls) { %>
			<div class="wall" data-wall-id="<%= wall.id %>">
				<p>id: <%= wall.id %></p>
				<p>name: <%= wall.name %></p>
				<p>difficulty: <%= wall.difficulty %></p>
				<p>location: <%= wall.location %></p>
				<p>date: <%= wall.date %></p>
				<p>setter: <%= wall.setter %></p>
				<p>color: <%= wall.color %></p>
				<p>status: <%= wall.active ? 'active' : 'inactive' %></p>
				<p>climbed: <input class="wall-climbed-checkbox" type="checkbox" <%= climbedWalls.indexOf(wall.id) !== -1 ? 'checked ' : '' %></p>
			</div>
			<br>
		<% } %>
	</div>
</body>
</html>
